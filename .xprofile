#!/bin/sh


## scripts
export PATH="$PATH:$(du "$HOME/.local/bin/scripts" | cut -f2 | paste -sd ':')"
## randr
xrandr --output eDP1 --primary --mode 1366x768 --pos 0x0 --rotate normal --output DP1 --off --output HDMI1 --off --output HDMI2 --off --output VIRTUAL1 --off
## Desktop Wallpaper
nitrogen --restore &
## Compton.
picom -cC &
## Music
mpd &
